{
  "extends": ["standard-with-typescript", "next/core-web-vitals"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": ["warn"],
    "no-console": "warn",
    "no-debugger": "warn",
    "comma-dangle": ["error", { "arrays": "always-multiline", "objects": "always-multiline" }]
  }
}
